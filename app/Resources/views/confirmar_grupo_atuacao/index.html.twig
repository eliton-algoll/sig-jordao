{% extends "::base.html.twig" %}

{% block subtitle %}Confimar Grupo Tutorial{% endblock %}

{% block body %}
    <div>
        <label>Grupo Tutorial</label>
        <select
            id="grupos"
            class="form-control">
            <option value=""></option>
            {% for grupo in projeto.getGruposAtuacaoAtivos() %}
                <option value="{{ grupo.getCoSeqGrupoAtuacao() }}">{{ grupo.getNoGrupoAtuacao() }}</option>
            {% endfor %}
        </select>
    </div>

    <div
        id="grid-grupo-tutorial"
        class="margin-top-20"></div>
{% endblock %}

{% block javascripts %}
    {% javascripts 'js/confirmar_grupo_atuacao/main.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}